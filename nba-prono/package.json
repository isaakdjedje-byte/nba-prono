{
  "name": "nba-prono",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/check-workspace-context.mjs && next dev",
    "build": "node scripts/check-workspace-context.mjs && next build",
    "start": "next start",
    "lint": "node scripts/check-workspace-context.mjs && eslint",
    "test": "node scripts/check-workspace-context.mjs && vitest run",
    "test:unit": "node scripts/check-workspace-context.mjs && vitest run",
    "test:smoke": "node scripts/check-workspace-context.mjs && node scripts/smoke-routes.mjs",
    "test:e2e": "playwright test",
    "test:e2e:p0": "playwright test --grep \"[P0]\"",
    "test:e2e:p1": "playwright test --grep \"[P0|P1]\"",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:api": "playwright test --project=api",
    "test:report": "playwright show-report",
    "contracts:check": "node scripts/check-workspace-context.mjs && node scripts/contracts-check.mjs",
    "contracts:sync": "node scripts/contracts-check.mjs --write"
  },
