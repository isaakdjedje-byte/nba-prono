    "pino": "10.3.1",
    "pino-http": "11.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "zod": "4.3.6",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@faker-js/faker": "9.0.0",
